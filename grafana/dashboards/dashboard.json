          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH top_repos AS (     SELECT         repository,         SUM(total_commits + total_pull_requests + merged_pull_requests) AS total_activities     FROM         public.fct_repository_activity     WHERE         EXTRACT(YEAR FROM month) = 2024      GROUP BY         repository     ORDER BY         total_activities DESC     LIMIT 3 )  \nSELECT     fa.month AS \"time\",     COALESCE(SUM(CASE WHEN fa.repository = tr1.repository THEN fa.total_commits + fa.total_pull_requests + fa.merged_pull_requests END), 0) AS top1_repository,     COALESCE(SUM(CASE WHEN fa.repository = tr2.repository THEN fa.total_commits + fa.total_pull_requests + fa.merged_pull_requests END), 0) AS top2_repository,     COALESCE(SUM(CASE WHEN fa.repository = tr3.repository THEN fa.total_commits + fa.total_pull_requests + fa.merged_pull_requests END), 0) AS top3_repository FROM     public.fct_repository_activity fa CROSS JOIN     (SELECT repository FROM top_repos ORDER BY total_activities DESC LIMIT 1 OFFSET 0) tr1,     (SELECT repository FROM top_repos ORDER BY total_activities DESC LIMIT 1 OFFSET 1) tr2,     (SELECT repository FROM top_repos ORDER BY total_activities DESC LIMIT 1 OFFSET 2) tr3 WHERE     fa.repository IN (SELECT repository FROM top_repos)     AND EXTRACT(YEAR FROM fa.month) = 2024 GROUP BY     fa.month ORDER BY     fa.month",
          "refId": "A",
          "sql": {
            "columns": [
